package com.engenharia.PoaAcademico.entity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="curso")
public class Curso {
	
	@Column(name="nome")
	private String curso;

	public String getCurso() {
		return curso;
	}

	public void setCurso(String curso) {
		this.curso = curso;
	}
	
	
}
