# Porta de escuta do servidor de aplicação
server.port=8083
# Comportamento do JPA em relação a estrutura do banco de dados
spring.jpa.hibernate.ddl-auto=update
# URL do banco de dados (MySQL neste caso) e o nome do database (schema)
spring.datasource.url=jdbc:mysql://localhost:3306/nome_do_database
# Usuário do banco de dados com permissões DDL
spring.datasource.username=root
# Senha do usuário do banco de dados
spring.datasource.password=
